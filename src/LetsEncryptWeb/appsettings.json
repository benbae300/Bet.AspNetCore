{
  "LetsEncryptDomainName": "acme.kingdavidconsulting.com",

  "LetsEncrypt": {
    "UseStagingServer": true
  },

  "AzureVault": {
    "BaseUrl": "https://bet.vault.azure.net/"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",

  "AzureStorage": {

    "DefaultAccount": {
      "Name": "betstorage"
    }
  },

  "StorageBlobs": {
    "certificates": {
      "ContainerName": "certificates"
    },

    "challenges": {
      "ContainerName": "challenges"
    }
  }
}
